<div class="fullheight container">
  <h2 class="text-title d-flex justify-content-center pt-4 pb-4">ジャンル編集</h2>
  <%= form_with model: [:admin,@genre] do |f| %>
    <div class="row mt-5">
      <div class="col-md-3"></div>
      <div class="text ml-5 mr-5">ジャンル名</div>
      <%= f.text_field :name, :class => "mr-5" %>
      <%= f.submit "変更を保存", :class => "btn btn-success" %>
    </div>
  <% end %>
</div>